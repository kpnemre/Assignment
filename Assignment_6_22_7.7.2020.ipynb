    "# Prime Number\n",
    "n = int(input(\"Enter a number please : \"))\n",
    "alist = []\n",
    "for i in range(2,n+1):\n",
    "    status= True\n",
    "    for j in range(2,i):\n",
    "        if i%j==0:\n",
    "            status = False\n",
    "    if status:\n",
    "         alist.append(i)\n",
    "       \n",
    "print(alist)\n",
